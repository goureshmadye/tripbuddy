{
  "name": "tripbuddy",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:coverage": "jest --coverage",
    "test:snapshots": "jest --testPathPattern=__tests__/screens --testNamePattern=snapshot",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "expo lint --fix",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security-check": "npm audit --audit-level=moderate",
    "security-scan": "npm audit --json | jq '.vulnerabilities | keys[]'",
    "build:android": "eas build --platform android --profile preview --non-interactive",
    "build:ios": "eas build --platform ios --profile preview --non-interactive",
    "build:production": "eas build --platform all --profile production --non-interactive",
    "submit:android": "eas submit --platform android --profile production --non-interactive",
    "submit:ios": "eas submit --platform ios --profile production --non-interactive",
    "release": "npm run build:production && npm run submit:android && npm run submit:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@google/genai": "^1.34.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.30",
    "expo-auth-session": "^7.0.10",
    "expo-blur": "~15.0.8",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.12",
    "expo-crypto": "^15.0.8",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-router": "~6.0.21",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "firebase": "^12.7.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-purchases": "^9.7.1",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-razorpay": "^2.3.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@react-native-community/cli": "latest",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.1.0",
    "audit-ci": "^7.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "expo-module-scripts": "^5.0.8",
    "jest": "~29.7.0",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "qs": "^6.14.1"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-razorpay"
        ]
      }
    },
    "install": {
      "exclude": [
        "react-native-razorpay"
      ]
    }
  },
  "private": true
}
